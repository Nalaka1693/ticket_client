package service;

public class AvailabilityResponse {
	int reservationId;
	String originCity;
	String destinationCity;
	int numOfTickets;
	int totalPrice;
	
	public AvailabilityResponse(String originCity, String destinationCity, int numOfTickets) {
		this.originCity = originCity;
		this.destinationCity = destinationCity;
		this.numOfTickets = numOfTickets;
	}

}
